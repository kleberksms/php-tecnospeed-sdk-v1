{
    "name": "tecnospeed/tecnospeed-sdk-v1",
    "description": "Tecnospeed Manager eDoc SDK for PHP",
    "keywords": ["Tecnospeed", "sdk"],
    "type": "library",
    "homepage": "https://github.com/kleberksms/php-tecnospeed-sdk-v1",
    "license": "",
    "authors": [
        {
            "name": "Bellinati Perez",
            "homepage": "http://www.bellinatiperez.com.br"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-i18n": "2.*",
        "zendframework/zend-validator": "2.*",
        "zendframework/zend-crypt": "2.*",
        "respect/validation": "0.10.*@dev"
    },
    "minimum-stability":"dev",
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "guzzlehttp/guzzle": "~4.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "autoload": {
        "psr-4": {
            "Tecnospeed\\": "src/Tecnospeed/"
        }
    }
}
